<Window x:Class="ChemReagents.AdditionalWins.SettingsWin.SettingsWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChemReagents.AdditionalWins.SettingsWin"
        mc:Ignorable="d"
        Title="SettingsWin" Height="400" Width="600">
    <Grid>
        <CheckBox x:Name="DevModeBox" Content="Режим разработчика" HorizontalAlignment="Left" Margin="29,20,0,0" VerticalAlignment="Top" IsChecked="{Binding EnableDevMode}"/>
        <Button x:Name="button" Content="Сохранить" HorizontalAlignment="Left" Margin="29,339,0,0" VerticalAlignment="Top" Width="75" Command="{Binding SaveComm}"/>


    </Grid>
</Window>
