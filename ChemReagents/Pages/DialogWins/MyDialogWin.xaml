<Window x:Class="ChemReagents.Pages.DialogWins.MyDialogWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChemReagents.Pages.DialogWins"
        mc:Ignorable="d"
        Title="MyDialogWin" Height="450" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="60"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBlock x:Name="TextBl" Grid.ColumnSpan="2" Margin="10" TextWrapping="Wrap"/>
        <Button Grid.Row="1" Margin="10" x:Name="YesB" Content="Да" Click="YesB_Click"/>
        <Button Grid.Row="1" Grid.Column="1" Margin="10" x:Name="NoB" Content="Нет" Click="NoB_Click"/>

    </Grid>
</Window>
