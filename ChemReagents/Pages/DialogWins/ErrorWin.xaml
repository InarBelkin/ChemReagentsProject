<Window x:Class="ChemReagents.Pages.DialogWins.ErrorWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChemReagents.Pages.DialogWins"
        mc:Ignorable="d"
        Title="ErrorWin" Height="450" Width="450">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <TextBlock  x:Name="TextRus"/>
        <TextBlock Grid.Row="1"  x:Name="TextEng"/>
        <TextBlock Grid.Row="2"  x:Name="TextEng2"/>
        <Button Grid.Row="3"  Margin="10" Content="ОК" Click="Button_Click"/>

    </Grid>
</Window>
